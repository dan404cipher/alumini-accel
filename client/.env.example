# ========================================
# AlumniAccel Client Environment Variables
# ========================================
# Copy this file to .env and fill in your actual values

# ========================================
# API Configuration
# ========================================
VITE_API_BASE_URL=http://localhost:3000/api/v1
VITE_API_TIMEOUT=10000

# ========================================
# Authentication
# ========================================
VITE_JWT_STORAGE_KEY=alumni_accel_token
VITE_REFRESH_TOKEN_KEY=alumni_accel_refresh_token

# ========================================
# External Services
# ========================================
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_LINKEDIN_CLIENT_ID=your-linkedin-client-id

# ========================================
# Analytics (Optional)
# ========================================
VITE_GOOGLE_ANALYTICS_ID=your-ga-id
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# ========================================
# Feature Flags
# ========================================
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_FILE_UPLOAD=true

# ========================================
# Development
# ========================================
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info

# ========================================
# App Configuration
# ========================================
VITE_APP_NAME=AlumniAccel
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=The Ultimate Alumni Engagement & Empowerment Platform

# ========================================
# UI Configuration
# ========================================
VITE_THEME=light
VITE_LANGUAGE=en
VITE_TIMEZONE=UTC

# ========================================
# File Upload
# ========================================
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# ========================================
# Real-time Features
# ========================================
VITE_SOCKET_URL=ws://localhost:3000
VITE_ENABLE_REALTIME=true

# ========================================
# Performance
# ========================================
VITE_ENABLE_CACHE=true
VITE_CACHE_DURATION=3600
